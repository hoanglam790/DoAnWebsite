
@model WebsiteDuLichDiaPhuong.Models.DIADANH
@{
    ViewBag.Title = "ChiTiet";
    Layout = "~/Views/Shared/_LayoutNguoiDung.cshtml";
}

<h2 style="text-align: center; color: cornflowerblue; padding-top: 40px"> CHI TIẾT ĐỊA DANH </h2>
<div class="container-fluid pt-5">

    <p style="color: brown; padding-top: 40px; margin-left: 80px"><span style="font-weight: bold"> Tên địa danh: </span> @Model.TenDiaDanh </p>
    <p style="color: brown; padding-top: 10px; margin-left: 80px"><span style="font-weight: bold"> Huyện: </span> @Model.HUYEN.TenHuyen </p>
    <a href="" class="cat-img position-relative overflow-hidden mb-3">
        <img class="img-fluid" src="~/Images/@Model.HINHANH.TenHinhAnh" style="max-width: 100%; padding-top: 10px; margin-left: 80px" alt="">
    </a>
    <p style="text-align: justify; margin-left: 80px; padding-top: 30px">@Model.GioiThieu</p>

</div>

