<!-- Hiển thị các tin tức nổi bật - Bắt đầu -->
<div class="container-fluid pt-5">
    <div style="margin-left: 48px; margin-right: 48px; background-color: pink" class="cat-item d-flex flex-column border text-center mb-4">
        <h2 style="text-align: center; color: black; font-size: 26px; font-weight: bold; padding-top: 20px; margin-left: 60px"> MỘT SỐ TIN TỨC NỔI BẬT</h2>
    </div>
    <div class="row px-xl-5 pb-3">
        @foreach (var item in Model)
        {
            <div class="col-lg-4 col-md-6 pb-1">
                <div class="cat-item d-flex flex-column border mb-4" style="padding: 30px;">
                    <p class="text-center">@item.TieuDe</p>
                    <a href="" class="cat-img position-relative overflow-hidden mb-3">
                        <img class="img-fluid" src="~/Images/@item.HINHANH.TenHinhAnh" style="width: 400px; height: 250px" alt="">
                    </a>
                </div>
            </div>
        }
    </div>
</div>
